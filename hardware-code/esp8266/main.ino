void updateDHTIfNeeded() {
  if (millis() - lastDHTRead >= 2000) {
    yield();
    float t = dht.readTemperature();
    float h = dht.readHumidity();
    yield();
    if (!isnan(t) && !isnan(h)) {
      cachedTemp = t;
      cachedHum = h;
    }
    lastDHTRead = millis();
  }
}


