  if (!wifiConnected) {
    showInfo("Starting WiFiManager");
    WiFiManagerParameter custom_utc("utc", "UTC Offset", String(config.utcOffset).c_str(), 3);
    display.clearDisplay();
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.print("WifiManager Started.");
    display.setCursor(0, 20);
    display.print("SSID: NanaClock Setup");
    display.setCursor(0, 40);
    display.print("IP: 192.168.4.1 ");
    display.display();
    wm.addParameter(&custom_utc);
    if (!wm.autoConnect("NanaClock Setup")) {
      showInfo("WiFiManager fail. Restart...");
      ESP.restart();
    }
    config.ssid = WiFi.SSID();
    config.pass = WiFi.psk();
    config.utcOffset = String(custom_utc.getValue()).toInt();
    saveConfig();
  }
  showInfo("WiFi OK: " + WiFi.SSID());
  delay(4000);

  // NTP
  timeClient = new NTPClient(ntpUDP, "pool.ntp.org", 3600 * config.utcOffset, 60000);
  timeClient->begin();

  // Web server
  server.on("/", handleRoot);
  server.on("/settime", handleSetTime);
  server.on("/gettime", handleGetTime);
  server.on("/buzztime", handleBuzzTime);
  server.on("/setwifi", handleSetWiFi);
  server.on("/getwifi", handleGetWiFi);
  server.on("/bitmapwindow", handleBitmapWindow);
	server.on("/ipaddress", handleIpAddress);
  server.begin();
  showInfo("Nana Ready! IP:" + WiFi.localIP().toString());
  delay(5000);

  display.clearDisplay();
  display.setTextSize(1);
  display.setCursor(0, 0);
  display.print("Testing Buzzer");
  display.display();
  buzzNow("merry");
  delay(500);
  display.clearDisplay();
  display.setTextSize(1);
  display.setCursor(0, 0);
  display.print("Okai !");
  display.display();
  delay(4000);

}

// --- Loop ---
unsigned long lastBuzz = 0;
bool hasBuzzedThisMinute = false;

void loop() {
  server.handleClient();
  timeClient->update();

  int h = timeClient->getHours();
  int m = timeClient->getMinutes();

  char curTime[6];
  snprintf(curTime, sizeof(curTime), "%02d:%02d", h, m);

  // Buzz logic
  bool shouldBuzz = false;
  String songToPlay = "";
  for (int i = 0; i < MAX_BUZZ; i++) {
    if (config.buzzTimes[i].time == String(curTime)) {
      shouldBuzz = true;
      songToPlay = config.buzzTimes[i].song;
    }
  }
  if (shouldBuzz && !hasBuzzedThisMinute) {
    buzzNow(songToPlay.length() ? songToPlay : "hedwig");
    hasBuzzedThisMinute = true;
    lastBuzz = millis();
  }
  if (!shouldBuzz || (millis() - lastBuzz > 60000)) {
    hasBuzzedThisMinute = false;
  }

  // Animated bitmap window
  bool showBitmap = (m >= config.bitmapWindowStart && m < config.bitmapWindowEnd);
  if (showBitmap) {
    animateBitmap();
    return;
  }

  // Show time
  showTime();
  delay(1000);
}