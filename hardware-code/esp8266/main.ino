  Serial.print("DHT read attempt. Temp: ");
  Serial.print(t);
  Serial.print(" C, Humidity: ");
  Serial.print(h);
  Serial.println(" %");

  if (!isnan(t) && !isnan(h)) {
    String response = "{\"temperature\":" + String(t, 1) + ",\"humidity\":" + String(h, 1) + "}";
		server.sendHeader("Access-Control-Allow-Origin", "*");
