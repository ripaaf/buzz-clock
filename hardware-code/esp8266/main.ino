void playMelody(int* melody, int noteCount, int tempo) {
  int wholenote = (60000 * 4) / tempo;
  for (int i = 0; i < noteCount * 2; i += 2) {
    int note = melody[i];
    int durationCode = melody[i + 1];

    int noteDuration = wholenote / abs(durationCode);
    if (durationCode < 0) {
      noteDuration = noteDuration * 1.5;
    }

    if (note != REST) {
      tone(BUZZER_PIN, note, noteDuration * 0.9);
    }

    delay(noteDuration);
    noTone(BUZZER_PIN);
  }
}

// Unified function to play songs
void buzzNow(String songName) {
  if (songName == "hedwig") {
    Serial.println("Playing: Hedwig's Theme");
    int noteCount = sizeof(melodyHedwig) / sizeof(melodyHedwig[0]) / 2;
    playMelody(melodyHedwig, noteCount, tempoHedwig);
  } else if (songName == "merry") {
    Serial.println("Playing: We Wish You a Merry Christmas");
    int noteCount = sizeof(melodyMerry) / sizeof(melodyMerry[0]) / 2;
    playMelody(melodyMerry, noteCount, tempoMerry);
  } else {
    Serial.println("Unknown song: " + songName);
  }
}

